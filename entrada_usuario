// ENTRADA USUARIO 
import java.util.Scanner;
class Main{
 public static void main(String[] args){
 
  // Se crea un Objeto de la clase Scanner
  Scanner input = new 
  Scanner(System.in);
  
  System.out.println("Enter an Integer: ");
  
  // recibir entrada(Entero) del usuario 
  int data1 = input.nextInt();
  
  System.out.println("Enter a Double: ");
  
  // recibir entrada(Double) del usuario 
  Double data2 = input.nextDouble();
  
  System.out.println("Enter Text: ");
  
  // recibir entrada(String) del usuario 
  String data3 = input.nextLine();
  
  System.out.println("Input Data Int: " + data1);
  System.out.println("Input Data Double: " + data2);
  System.out.println("Input Data String: " + data3);
  
  // cerrar Scanner
  input.close();
  }
}
